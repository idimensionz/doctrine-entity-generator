{
    "name": "idimensionz/doctrine-entity-generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Jim Mullen",
          "email": "info@idimensionz.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.6",
        "symfony/cache": "^3.4",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/debug": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/doctrine-bridge": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/routing": "^3.4",
        "symfony/symfony": "^3.4",
        "symfony/twig-bridge": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "phake/phake": "^3.1",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "iDimensionz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iDimensionz\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "~3.4"
        }
    }
}
